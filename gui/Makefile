.obj/tkgeomgui.o: tkgeomgui.cpp .ui/maindialog.h \
		inc/filehandler.h \
		inc/gui_constants.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/tkgeomgui.o tkgeomgui.cpp

.obj/FileHandler.o: src/FileHandler.cc inc/filehandler.h \
		inc/gui_constants.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/FileHandler.o src/FileHandler.cc

.ui/maindialog.h: maindialog.ui inc/filehandler.h \
		inc/gui_constants.h
	$(UIC) maindialog.ui -o .ui/maindialog.h

.ui/maindialog.cpp: .ui/maindialog.h maindialog.ui maindialog.ui.h inc/filehandler.h \
		inc/gui_constants.h
	$(UIC) maindialog.ui -i maindialog.h -o .ui/maindialog.cpp

.obj/maindialog.o: .ui/maindialog.cpp maindialog.ui.h \
		.ui/maindialog.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/maindialog.o .ui/maindialog.cpp

.obj/moc_maindialog.o: .moc/moc_maindialog.cpp  .ui/maindialog.h inc/filehandler.h \
		inc/gui_constants.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/moc_maindialog.o .moc/moc_maindialog.cpp

.moc/moc_maindialog.cpp: $(MOC) .ui/maindialog.h
	$(MOC) .ui/maindialog.h -o .moc/moc_maindialog.cpp

