####### Build rules

all: Makefile $(TARGET)

$(TARGET):  $(UICDECLS) $(OBJECTS) $(OBJMOC)  
	$(LINK) $(LFLAGS) -o $(TARGET) $(OBJECTS) $(OBJMOC) $(OBJCOMP) $(LIBS)

mocables: $(SRCMOC)
uicables: $(UICDECLS) $(UICIMPLS)

$(MOC): 
	( cd $(QTDIR)/src/moc && $(MAKE) )

Makefile: tkgeomgui.pro  /usr/share/qt3/mkspecs/default/qmake.conf /usr/share/qt3/lib/libqt-mt.prl
	$(QMAKE) -o Makefile tkgeomgui.pro
qmake: 
	@$(QMAKE) -o Makefile tkgeomgui.pro

dist: 
	@mkdir -p .obj/tkgeomgui && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .obj/tkgeomgui/ && $(COPY_FILE) --parents maindialog.ui.h .obj/tkgeomgui/ && ( cd `dirname .obj/tkgeomgui` && $(TAR) tkgeomgui.tar tkgeomgui && $(GZIP) tkgeomgui.tar ) && $(MOVE) `dirname .obj/tkgeomgui`/tkgeomgui.tar.gz . && $(DEL_FILE) -r .obj/tkgeomgui

mocclean:
	-$(DEL_FILE) $(OBJMOC)
	-$(DEL_FILE) $(SRCMOC)

uiclean:
	-$(DEL_FILE) $(UICIMPLS) $(UICDECLS)

yaccclean:
lexclean:
clean: mocclean uiclean
