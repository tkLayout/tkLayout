// Generic services conversion rules
// Services for the standard rods
// (scales with respective material)
// SERVICES
// HV, MV, LV
Station {
  stationName FPX1
  type second
  minZ 330
  maxZ 420

  // 2*(2+1) TWP/module
  Conversion {
    Input {
      // Two conductors
      Element {
        elementName Cu_R1
        quantity 1.326
        unit g/m
      }
    }
    Output {
      @include-std CMS_Phase2/Pixel/Materials/GBT_and_fibres
    }   
  }

  // 7*(1+1) TWP/module
  Conversion {
    Input {
      // Two conductors
      Element {
        elementName Cu_R2
        quantity 3.094
        unit g/m
      }
    }
    Output {
      @include-std CMS_Phase2/Pixel/Materials/GBT_and_fibres
    }   
  }

  // 14*(1+1) TWP/module
  Conversion {
    Input {
      // Two conductors
      Element {
        elementName Cu_R3
        quantity 6.188
        unit g/m
      }
    }
    Output {
      @include-std CMS_Phase2/Pixel/Materials/GBT_and_fibres
    }   
  }

  Conversion {
    Input {
      // Two insulations
      Element {
        elementName Kapton
        quantity 0.057
        unit g/m
      }
    }
    Output {
    }
  }
}




