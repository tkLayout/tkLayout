// 1:1 conversion of CO2

Conversion {
  Input {
    Element {
      elementName CO2 
      quantity 1
      unit g/m
    }
  }
  Output {
    Element {
      elementName CO2
      quantity 1
      unit g/m
      service true
    }
  }
}
