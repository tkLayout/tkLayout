// One cooling pipe serves one rods' cooling pipes 

  // Titanium pipe
  Conversion {
    Input {
      Element {
        elementName Ti_L3 
        quantity 2
        unit g/m
      }
    }
    Output {
      Element {
        elementName Ti_L3
        quantity 1
        unit g/m
        service true
      }
    }  
  }
 

  // Coolant
  Conversion {
    Input {
      Element {
        elementName CO2_L3
        quantity 2
        unit g/m
      }
    }
    Output {
      Element {
        elementName CO2_L3
        quantity 1
        unit g/m
        service true
      }
    }  
  }
