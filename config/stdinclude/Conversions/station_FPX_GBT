// Generic services conversion rules
// Services for the standard rods
// (scales with respective material)
// SERVICES
// HV, MV, LV
Station {
  stationName FPX1
  type second
   minZ 330
   maxZ 420

// 2*(2+1) TWP/module
  Conversion {

    Input {
    // Two conductors
    Element {
      elementName Cu_R1
      quantity 1.326
      unit g/m
    }
  }

 Output {
@includestd Materials/GBT_and_fibres
}   
}





// 7*(1+1) TWP/module
  Conversion {

    Input {
    // Two conductors
    Element {
      elementName Cu_R2
      quantity 3.094
      unit g/m
    }
  }

 Output {
@includestd Materials/GBT_and_fibres
}   
}



