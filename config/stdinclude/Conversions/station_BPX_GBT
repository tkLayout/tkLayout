// Generic services conversion rules
// Services for the standard rods
// (scales with respective material)
// SERVICES
// HV, MV, LV
Station {
  stationName BPX1
  type second
minZ 300
   maxZ 390



// (6+1) TWP/module
  Conversion {

    Input {
    // Two conductors
    Element {
      elementName Cu
      quantity 1.547
      unit g/m
    }
  }

 Output {
@includestd Materials/GBT_and_fibres
}
}





  Conversion {
 Input {
    // Two insulations
    Element {
      elementName Kapton
      quantity 0.134
      unit g/m
    }
}
Output {
}
}
}








