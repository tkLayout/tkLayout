// Generic services conversion rules
// Services for the standard rods
// (scales with respective material)
// SERVICES
// HV, MV, LV
Station {
  stationName flangeFPX
  type flange



  // Manifolds + radial pipes + holders
  Conversion {
    Input {
      Element {
        elementName FPX_Steel
        quantity 1
        unit g/m
      }
    }
    Output {
      Element {
        elementName Steel
        quantity 1000
        unit g/m
        service true
      }
    }
  }

  Conversion {
    Input {
      Element {
        elementName FPX_CO2
        quantity 1
        unit g/m
      }
    }
    Output {
      Element {
        elementName CO2
        quantity 1000
        unit g/m
        service true
      }
    }
  }






// power wires
Conversion {
    Input {
      Element {
        elementName FPX_Al
        quantity 1
        unit g/m
      }
    }
    Output {
      Element {
        elementName Al
        quantity 1000
        unit g/m
        service true
      }
    }
  }

  Conversion {
    Input {
      Element {
        elementName FPX_PE
        quantity 1
        unit g/m
      }
    }
    Output {
      Element {
        elementName PE
        quantity 1000
        unit g/m
        service true
      }
    }
  }









  
}
