// BPX ROD material
// In rod service true means that exit from the rod
//        service false means that stay in the rod (but in the service cylinder)

Materials BPX_rod_L1 {
  type rod

   // Cooling for the module
  Component {
    componentName Cooling
    service true
    scaleOnSensor 0
    Element {
      elementName Steel
      quantity 5.15
      unit g/m
    }
    Element {
      elementName CO2
      quantity 3.14
      unit g/m
    }
  }





  // Power wires
  // (This is a bus line: service does not accumulate)
  Component {
    componentName "Power wires"
    service true
    scaleOnSensor 0
    // Two wires
    Element {
      elementName Cu
      quantity 0.78
      unit g/m
    }
    // Two wires
    Element {
      elementName PE
      quantity 0.62
      unit g/m
    }
    // Two wires 0.8 mm^2
    Element {
      elementName Al
      quantity 4.32
      unit g/m
    }
  }



  // HV Power wires
  // (This is a bus line: service does not accumulate)
  Component {
    componentName "HV Power wires"
    service true
    scaleOnSensor 0
    // Two wires
    Element {
      elementName Cu
      quantity 0.56
      unit g/m
    }
    // Two wires
    Element {
      elementName PE
      quantity 0.44
      unit g/m
    }
    // Two wires 0.4 mm^2
    Element {
      elementName Al
      quantity 2.16
      unit g/m
    }
  }

}


