      cotalphaLimit 0.1                  //if (cot(alpha) < cotalphaLimit), the 'Inf' parameters resolutionLocalXBarrelParamiInf are used, otherwise the 'Sup' parameters resolutionLocalXBarrelParamiSup are used.
      resolutionLocalXBarrelParam0Inf 0.0075  // mm
      resolutionLocalXBarrelParam1Inf 0.016    // mm
      resolutionLocalXBarrelParam2Inf 0.077      // mm
      resolutionLocalXBarrelParam0Sup 0.010  // mm
      resolutionLocalXBarrelParam1Sup 0.  // mm
      resolutionLocalXBarrelParam2Sup 0. // mm
