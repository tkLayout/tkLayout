// ROD material
// In rod service true means that exit from the rod
//        service false means that stay in the rod (but in the service cylinder)


   // Cooling for the module
  Component {
    componentName Cooling
    service true
    scaleOnSensor 0
    targetVolume 0
    Element {
      elementName Steel
      quantity 2.57
      unit g/m
    }
    Element {
      elementName CO2
      quantity 1.57
      unit g/m
    }
  }

 

  // 1 TWP/rod
  // (This is a bus line: service does not accumulate)
  Component {
    componentName "Twisted pairs"
    service true
    scaleOnSensor 0
    targetVolume 0
    // Two conductors and a drain
    Element {
      elementName Cu
      quantity 0.085
      unit g/m
    }
    // Two insulations
    Element {
      elementName PE
      quantity 0.049
      unit g/m
    }
    // Aluminium polyester foil
    Element {
      elementName Al
      quantity 0.083
      unit g/m
    }
  }

 


